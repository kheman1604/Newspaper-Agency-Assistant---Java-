<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane minHeight="800.0" minWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="connection.papermaster.papermasterFxmlController">
   <children>
      <ComboBox fx:id="txtNameCombo" editable="true" layoutX="106.0" layoutY="101.0" onAction="#doSelectComboValue" prefHeight="48.0" prefWidth="288.0" />
      <TextField fx:id="txtPrice" layoutX="106.0" layoutY="222.0" prefHeight="48.0" prefWidth="288.0" />
      <Label layoutX="106.0" layoutY="70.0" text="Paper Title">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="106.0" layoutY="184.0" text="Price">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="btnAdd" layoutX="54.0" layoutY="326.0" mnemonicParsing="false" onAction="#doAddNew" text="Add New" />
      <Button fx:id="btnsave" layoutX="171.0" layoutY="326.0" mnemonicParsing="false" onAction="#doSave" text="Save" />
      <Button fx:id="btnUpdate" layoutX="273.0" layoutY="326.0" mnemonicParsing="false" onAction="#doUpdate" text="Update" />
      <Button fx:id="btnRemove" layoutX="384.0" layoutY="326.0" mnemonicParsing="false" onAction="#doRemove" text="Remove" />
   </children>
</AnchorPane>
